{
  "name": "rest-express",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "NODE_ENV=development tsx server/index.ts",
    "build": "vite build && esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "start": "NODE_ENV=production node dist/index.js",
    "start:test": "NODE_ENV=test node dist/index.js",
    "check": "tsc",
    "type-check": "tsc --noEmit --strict",
    "type-check:detailed": "tsx scripts/type-check.ts",
    "phase1:status": "tsx scripts/phase1-status.ts",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format:check": "prettier --check .",
    "format:write": "prettier --write .",
    "db:push": "drizzle-kit push",
    "db:migrate:test": "drizzle-kit push --config=drizzle.test.config.ts",
    "db:seed:test": "tsx scripts/seed-test-data.ts",
    "db:seed:e2e": "tsx scripts/seed-e2e-data.ts",
    "db:seed:performance": "tsx scripts/seed-performance-data.ts",
    "prepare": "husky",
    "test": "vitest run",
    "test:unit": "vitest run tests/unit",
    "test:unit:coverage": "vitest run tests/unit --coverage",
    "test:unit:watch": "vitest tests/unit",
    "test:integration": "vitest run tests/integration",
    "test:integration:coverage": "vitest run tests/integration --coverage",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:performance": "node scripts/run-performance-tests.js",
    "test:performance:api": "autocannon --renderStatusCodes --renderLatencyTable http://localhost:5000/api/health",
    "test:performance:websocket": "k6 run tests/performance/load/websocket-load.js",
    "test:performance:stress": "k6 run tests/performance/stress/stress-test.js",
    "test:performance:cache": "tsx tests/performance/cache-performance.test.ts",
    "test:performance:load": "tsx tests/performance/load-scaling.test.ts",
    "test:performance:suite": "tsx scripts/run-performance-suite.ts",
    "benchmark:targets": "tsx scripts/benchmark-performance-targets.ts",
    "validate:phase3": "tsx scripts/validate-phase3.ts",
    "test:performance:full": "tsx scripts/run-performance-suite.ts",
    "test:security": "vitest run tests/security",
    "test:security:full": "npm run test:security && npm run security:audit",
    "test:coverage": "vitest run --coverage",
    "test:coverage:report": "vitest run --coverage && open coverage/index.html",
    "test:watch": "vitest",
    "test:hooks": "vitest run tests/unit/hooks",
    "test:components": "vitest run tests/unit/components",
    "test:middleware": "vitest run tests/unit/middleware",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "test:scaling": "tsx server/websocket/scaling/test-runner.ts",
    "test:scaling:ci": "tsx server/websocket/scaling/test-runner.ts ci",
    "test:scaling:basic": "tsx server/websocket/scaling/test-runner.ts basic",
    "test:scaling:load-balancing": "tsx server/websocket/scaling/test-runner.ts load-balancing",
    "test:scaling:failure-recovery": "tsx server/websocket/scaling/test-runner.ts failure-recovery",
    "test:scaling:stress": "tsx server/websocket/scaling/test-runner.ts stress",
    "test:scaling:monitoring": "tsx server/websocket/scaling/test-runner.ts monitoring",
    "test:scaling:benchmark": "tsx server/websocket/scaling/test-runner.ts benchmark",
    "test:scaling:docker": "bash scripts/run-scaling-tests.sh test",
    "scaling:build": "bash scripts/run-scaling-tests.sh build",
    "scaling:start": "bash scripts/run-scaling-tests.sh start",
    "scaling:cleanup": "bash scripts/run-scaling-tests.sh cleanup",
    "security:audit": "npm audit --audit-level=high",
    "security:fix": "npm audit fix",
    "quality:check": "npm run lint && npm run type-check && npm run test:unit:coverage",
    "quality:gate": "node scripts/quality-gate-check.js",
    "quality:validate": "node scripts/quality-gate-validate.js",
    "ci:test": "npm run quality:check && npm run test:integration && npm run test:security",
    "ci:e2e": "npm run test:e2e",
    "ci:performance": "npm run test:performance",
    "ci:full": "npm run ci:test && npm run ci:e2e && npm run ci:performance"
  },
